  "license": "MIT",
  "dependencies": {
    "ampersand-app": "^1.0.4",
    "ampersand-router": "^3.0.2",
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "file-loader": "^0.8.4",
    "hjs-webpack": "^2.14.1",
    "json-loader": "^0.5.3",
    "postcss-loader": "^0.6.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.12.4",
    "stylus-loader": "^1.4.0",
    "surge": "^0.15.0",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "yeticss": "^7.0.5"
  },
  "jsintConfig": {
    "browser": true,
    "jquery": true,
    "node": true,
    "esnext": true,
    "camelcase": true,
    "eqeqeq": true,
    "indent": 2,
    "latedef": true,
    "maxlen": 80,
    "newcap": true,
    "quotmark": "single",
    "strict": true,
    "undef": true,
    "unused": true,
    "eqnull": true,
    "asi": false,
    "expr": true,
    "loopfunc": true,
    "evil": true,
    "curly": false,
    "white": true
