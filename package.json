{
  "name": "prodomo",
  "version": "0.0.3",
  "description": "Prodomo web frontend",
  "main": "index.js",
  "repository": "git@bitbucket.org:giupo/prodomo.git",
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild": "rm -rf public && mkdir public",
    "build": "NODE_ENV=production webpack",
